Cell[BoxData[
 RowBox[{
  RowBox[{"(", 
   RowBox[{"edgeData", "=", "\n", 
    RowBox[{
     RowBox[{"ParallelMap", "[", 
      RowBox[{
       RowBox[{
        RowBox[{"<|", 
         RowBox[{
          RowBox[{"\"\<Edge\>\"", "->", 
           RowBox[{"UndirectedEdge", "@@", "#"}]}], ",", 
          RowBox[{"\"\<RDKitTanimotoSimilarity\>\"", "->", "\n", "\t", 
           RowBox[{
            RowBox[{
            "ResourceFunction", "[", "\"\<MoleculeFingerprintSimilarity\>\"", 
             "]"}], "@@", 
            RowBox[{"nodeData", "[", 
             RowBox[{"#", ",", "\"\<Molecule\>\""}], "]"}]}]}], ",", "\n", 
          "\t", 
          RowBox[{"\"\<MCSTanimotoSimilarity\>\"", "->", 
           RowBox[{"mcsSimilarity", "@@", 
            RowBox[{"nodeData", "[", 
             RowBox[{"#", ",", "\"\<Molecule\>\""}], "]"}]}]}]}], "|>"}], 
        "&"}], ",", 
       RowBox[{"(*", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{
           RowBox[{"Take", "[", 
            RowBox[{"#", ",", "500"}], "]"}], "&"}], ")"}], "@"}], "*)"}], 
       "\n", "  ", 
       RowBox[{"Subsets", "[", 
        RowBox[{
         RowBox[{"Normal", "@", 
          RowBox[{"Keys", "@", "nodeData"}]}], ",", 
         RowBox[{"{", "2", "}"}]}], "]"}]}], "]"}], "//", "Dataset"}]}], 
   ")"}], "//", "AbsoluteTiming"}]], "Code",
