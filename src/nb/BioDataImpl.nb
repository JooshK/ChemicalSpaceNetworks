     RowBox[{"7", "<=", "pKi", "<", "8"}], ",", "Yellow", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"8", "<=", "pKi", "<", "9"}], ",", 
     RowBox[{"Darker", "@", "Green"}], ",", "\[IndentingNewLine]", 
     RowBox[{"9", "<=", "pKi", "<", "10"}], ",", "LightBlue", ",", 
     "\[IndentingNewLine]", 
     RowBox[{"10", "<=", "pKi"}], ",", "Blue", ",", "True", ",", "White"}], 
    "]"}]}], ";"}]], "Input",
 CellChangeTimes->{{3.897504212368429*^9, 3.897504298329755*^9}},
