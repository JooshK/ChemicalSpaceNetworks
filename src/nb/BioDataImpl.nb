 RowBox[{
  RowBox[{
   RowBox[{"defualtpKiToColor", "[", "pKi_", "]"}], ":=", 
   RowBox[{"Which", "[", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"pKi", "<", "5"}], ",", 
